{
  "compilerOptions": {
    "allowJs": false,
    "lib": ["es7"],
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "charset": "utf-8",
    "pretty": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "outDir": "dest"
  },
  "include": [
    "fsm.ts"
  ]
}
